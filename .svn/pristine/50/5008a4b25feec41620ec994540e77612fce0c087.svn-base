* {
	padding: 0;
	list-style: none;
	text-decoration: none;
	color: black;
	font-family: 'Nanum Gothic', sans-serif;
}

#wrap {
	margin: 0 auto;
	width: 1600px;
	height: 100%;
}


/* header */
/* header */
#wrap > header {
	width: 1600px;
	z-index: 100;
	height: 130px;
	background-color: white;
}

#wrap .logo {
	width: 300px;
	height: 200px;
	position: relative;
	top: -20px;
	left: 100px;
}

#wrap .nav {
	display: flex;
}

#wrap .nav li a {
	color: black;
	font-weight: bold;
	font-size: 17px;
	margin-right: 100px;
	position: relative;
	left: 500px;
	top: -110px;
}
  
  .content {
  clear: both;
  width: 100%;
}
  
  /* content */
  
  .content_title{
    position: relative;
    width: 800px;
    font-size: 40px;
    top: 30px;
    margin: 0 auto;
  }
  
  .service{
    height: 300px;
    background-color: gold;
  }
  
  .accoArea {
    width: 800px;
    height: 300px;
    margin: 0 auto;
  }
  
  .accoArea{display:block;box-sizing:border-box; padding-top:50px; height:auto; overflow:hidden;}
  .accoArea .accTit{display:block;background:url('../images/arrow_down.png') no-repeat 98% center; border-top:2px outset #333; margin-top:-1px;padding:10px 0;width:100%; font-size:25px;font-weight:500;text-indent:3%;color:#20253a;}
  .accoArea .accTit.on{background:url('../images/arrow_up.png') no-repeat 98% center;}
  .accoArea .accBox{display:none;}
  .accoArea .accBox:last-child{border-bottom:2px outset #333;}
  .accoArea .accBox ul{margin:0 auto;padding:10px 0;width:95%;}
  .accoArea .accBox li{position:relative;padding-left:10px; margin-bottom:10px;font-size: 20px;}
