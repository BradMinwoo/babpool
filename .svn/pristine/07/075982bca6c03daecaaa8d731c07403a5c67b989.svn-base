    float: left;
  }

  #myChat > #userImg{
    width: 50px;
    height: 50px;
    border-radius: 50px;
  }
  
  /* userChat */
  #userChat{
    float: left;
    clear: both;
  }

  #userChat > #userImg{
    width: 50px;
    height: 50px;
    border-radius: 50px;
  }
