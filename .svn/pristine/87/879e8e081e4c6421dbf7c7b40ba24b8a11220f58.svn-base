* {
	padding: 0;
	list-style: none;
	text-decoration: none;
	color: black;
	font-family: 'Nanum Gothic', sans-serif;
}

#wrap {
	margin: 0 auto;
	width: 1600px;
	height: 100%;
}


/* header */
header {
	width: 1600px;
	z-index: 100;
	height: 130px;
	background-color: white;
}

.logo {
	width: 300px;
	height: 200px;
	position: relative;
	top: -20px;
	left: 100px;
}

.nav {
	display: flex;
}

.nav li a {
	color: black;
	font-weight: bold;
	font-size: 17px;
	margin-right: 100px;
	position: relative;
	left: 500px;
	top: -110px;
}

/* content */

.content {
  clear: both;
  width: 100%;
}

table {
  border-collapse: collapse;
  border-spacing: 0;
}

section.noticewrite {
  padding: 80px 0;
}

.page-title {
  margin-bottom: 60px;
}
.page-title h3 {
  font-size: 28px;
  color: #333333;
  font-weight: 400;
  text-align: center;
}

#editor_form {
  width: 800px;
  transform: translate(50%);
}

#div_table {
  text-align: center;
  font-size: 15px;
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
}

#div_table th, #div_table td {
  padding: 14px 0;
}

#div_table tbody td {
  border-top: 1px solid #e7e7e7;
  text-align: center;
}

#div_table tbody th {
  padding-left: 28px;
  padding-right: 14px;
  border-top: 1px solid #e7e7e7;
  text-align: left;
}

#div_table tbody th p{
  display: none;
}

#div_table{
  height: 480px;
}

#div_table .td_left{
  text-align: center;
}
.td_right > input{
  width: 100%;
  height: 25px;
  border: none;
  border-bottom: 1px solid #999;
}

.td_right > textarea{
  width: 100%;
  height: 300px;
  border-radius: 5px;
  border: none;
  background: rgba(0, 0, 0, 0.1);
  font-size: 15px;
}

#div_button{
  height: 100px;
  margin-top: 150px;
  margin-right: 20px;
}

#div_button input{
  padding: 5px; 
  border: none;
  border-radius: 5px;
  margin-left: 10px;
  position: relative;
  bottom: 100px;
}

#div_button{
  text-align: right;
}

/* 버튼 액션추가 */
#div_button > input:active{
  transform:translate(2px,2px);
}

/* reset */

* {
  list-style: none;
  text-decoration: none;
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
.clearfix:after {
  content: '';
  display: block;
  clear: both;
}
.container {
  width: 1100px;
  margin: 0 auto;
}
.blind {
  position: absolute;
  overflow: hidden;
  clip: rect(0 0 0 0);
  margin: -1px;
  width: 1px;
  height: 1px;
}


/* footer */
.footer {
	clear: both;
	height: 150px;
	background-color: #FEF5ED;
}

.footercontent {
	left: 300px;
}

.logo_title {
	position: relative;
	font-size: 25px;
	text-align: center;
	font-weight: bold;
}

.footeraddr {
	position: relative;
	font-size: 15px;
	top: 50px;
	text-align: center;
}

.footeraddr>p {
	margin-top: 15px;
}

.sns_section {
	position: relative;
	text-align: center;
	top: 30px;
}

.sns_section>a {
	margin-left: 10px;
}

