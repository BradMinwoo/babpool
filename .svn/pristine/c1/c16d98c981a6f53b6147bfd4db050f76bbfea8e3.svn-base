* {
	margin: 0 auto;
	padding: 0;
	list-style: none;
	text-decoration: none;
	color: black;
	font-family: 'Jua', sans-serif;
}

#wrap {
	margin: 0 auto;
	width: 1600px;
	height: 100%;
}

/* header */
header {
	width: 1600px;
	height: 180px;
	background-color: white;
}

.info {
  position: relative;
  left: 1200px;
  display: flex;
  width: 200px;
  top: 20px;
}

.info > li {
  color: gray;
  padding: 10px;
}

.logo {
	width: 200px;
	float: left;
	position: relative;
   left: 170px;
   top: 15px;
}

#wrap .nav {
	width: 1200px;
	display: flex;
}

#wrap .nav li a {
	z-index: 100px;
	position: relative;
	left: 300px;
	font-size: 20px;
	list-style: none;
	text-decoration: none;
	padding: 30px;
	line-height: 200px;
}
/* header e */

.wordCloud{
	width:90%;
	height:500px;
}
.wordCloud{
	margin:0 auto;
	margin-bottom:50px;
}
.wordCloudWrap >img{
	height:500px;
	width:500px
}
.wordCloudWrap > p{
	font-size:30px;
	}

.recommendWrap{
	width:90%;
	height:500px;
	margin:0 auto;
	padding:0;
	
}

.recommendList{
	margin:0 auto;
	width:90%;
	height : 260px;
	margin-top:40px;
	display: flex;
}

.recommendItem{
	display:block;
/* 	width:200px; */
/* 	flex-basis: 200px; */
	/* max-height:200px; */
/* 	background:yellow; */
	margin-right:50px;
	/* float:left; */
	text-align:center;
	flex : 0 0 200px;
	border : 2px solid #000;
	border-radius : 10px;
	
}
.recommendItem > img{
	width : 100%;
	border-radius: 0 0 10px 10px;
	
}
.re_content{
	height : 50px;
	font-size: 13px;
	padding: 5px;
}
.button_tag {
    float: right;
    margin-right: 10%;
}
.btn_regist{
	border : none;
	background-color:transparent;
   font-size: 16px;
}



















/* footer */
.footer {
	clear: both;
	height: 150px;
	background-color: #FEF5ED;
}

.footercontent {
	left: 300px;
}

.logo_title {
	position: relative;
	font-size: 25px;
	text-align: center;
	font-weight: bold;
}

.footeraddr {
	position: relative;
	font-size: 15px;
	top: 50px;
	text-align: center;
}

.footeraddr>p {
	margin-top: 15px;
}

.sns_section {
	position: relative;
	text-align: center;
	top: 30px;
}

.sns_section>a {
	margin-left: 10px;
}

