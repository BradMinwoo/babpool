  top: 20px;
}

.info > li {
  color: gray;
  padding: 10px;
}

.logo {
	width: 200px;
	float: left;
	position: relative;
   left: 170px;
   top: 15px;
}

#wrap .nav {
	width: 1200px;
	display: flex;
}

#wrap .nav li a {
	z-index: 100px;
	position: relative;
	left: 300px;
	font-size: 20px;
	list-style: none;
