}

#a > .nameaaaaaa {
	display: none;
}

/* header */
header {
	top: 0;
	width: 1600px;
	position: fixed;
	z-index: 100;
	height: 180px;
	background-color: white;
}

.info {
  position: relative;
  left: 1200px;
