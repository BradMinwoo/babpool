* {
    margin: 0;
    padding: 0;
    list-style: none;
    text-decoration: none;
    color: black;
    font-family: 'Nanum Gothic', sans-serif;
}

#wrap {
	margin: 0 auto;
    width: 1600px;
    height: 100%;
}

/* header */
header {
	width: 1600px;
	height: 130px;
}

.logo {
	width: 300px;
	height: 200px;
	position: relative;
	top: -20px;
	left: 100px;
}

.nav {
    display: flex;
}

.nav li a {
	font-weight: bold;
	font-size: 17px;
	margin-right: 100px;
	position: relative;
	left: 500px;
	top: -110px;
	list-style: none;
	text-decoration: none;
}

/* header e */

.content {
    clear: both;
    padding: 80px 0;
}


table {
    border-collapse: collapse;
    border-spacing: 0;
}


#div_table {
    text-align: center;
    font-size: 15px;
}

#div_table th,
#div_table td {
    padding: 14px 0;
}

#div_table tbody td {
    border-bottom: 1px solid #adadad;
    text-align: center;
}

#div_table tbody th {
    padding-left: 28px;
    padding-right: 14px;
    border-top: 1px solid #e7e7e7;
    text-align: left;
}

#div_table tbody th p {
    display: none;
}

#div_table .td_left {
    background-color: #e7e7e7;
    text-align: center;
    font-weight: bold;
}

.td_right>input {
    font-size: 18px;
    width: 800px;
    height: 25px;
    border: none;
    padding: 10px;
}

.form-control {
    padding-left: 10px;
}

.td_right>textarea {
    padding: 10px;
    width: 100%;
    height: 300px;
    border-radius: 5px;
    border: none;
    font-size: 18px;
}

#div_button {
    height: 100px;
    margin-top: 150px;
    margin-right: 200px;
}

#div_button input {
    padding: 10px;
    border: none;
    border-radius: 5px;
    position: relative;
    bottom: 100px;
    font-size: 15px;
}

#div_button {
    text-align: right;
}

#file_title,
#file_area>input {
    border-bottom: none;
}

.form-inline {
    float: left;
}

/* 버튼 액션추가 */
#div_button>input:active {
    transform: translate(2px, 2px);
}


/* footer */
.footer {
    height: 150px;
    background-color: #FEF5ED;
}

.footercontent {
    left: 300px;
}

.logo_title {
    position: relative;
    font-size: 25px;
    text-align: center;
    font-weight: bold;
}

.footeraddr {
    position: relative;
    font-size: 15px;
    top: 50px;
    text-align: center;
}

.footeraddr>p {
    margin-top: 15px;
}

.sns_section {
    position: relative;
    text-align: center;
    top: 30px;
}

.sns_section>a {
    margin-left: 10px;
}

/* footer e */